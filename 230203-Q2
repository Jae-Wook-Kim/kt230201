#1
def solution(numbers):
    answer = 0
    for i in range(0,10):
        if i not in numbers:
            answer += i
    return answer

numbers = [1,2,3,4,6,7,8,0]
answer = solution(numbers)
print(answer)

#2
def solution(numbers):
    answer = sum(range(0,10)) - sum(numbers)
    return answer

numbers = [5,8,4,0,6,7,9]
answer = solution(numbers)
print(answer)
